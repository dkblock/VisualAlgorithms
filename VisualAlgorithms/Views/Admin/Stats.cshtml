@using System.Linq
@model IEnumerable<VisualAlgorithms.Models.UserTest>
@{
    ViewData["Title"] = "Статистика";
}

<div class="main-form-title text-center">Статистика</div>
<table class="table text-center">
    <thead>
    <tr>
        <th>@Html.DisplayNameFor(modelItem => modelItem.User.LastName)</th>
        <th>@Html.DisplayNameFor(modelItem => modelItem.User.FirstName)</th>
        <th>@Html.DisplayNameFor(modelItem => modelItem.User.Group)</th>
        <th>@Html.DisplayNameFor(modelItem => modelItem.Test.Name)</th>
        <th>@Html.DisplayNameFor(modelItem => modelItem.Test.Algorithm.Name)</th>
        <th>
            <select>

            </select>
            @Html.DisplayNameFor(modelItem => modelItem.Result)
        </th>
        <th>@Html.DisplayNameFor(modelItem => modelItem.PassingTime)</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.User.LastName</td>
            <td>@item.User.FirstName</td>
            <td>@item.User.Group</td>
            <td>@item.Test.Name</td>
            <td>@item.Test.Algorithm.Name</td>
            <td>@item.Result</td>
            <td>@item.PassingTime</td>
        </tr>
    }
    </tbody>
</table>